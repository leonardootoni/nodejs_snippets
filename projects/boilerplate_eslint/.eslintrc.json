{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb", "plugin:import/errors"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "import"],
  "rules": {}
}
